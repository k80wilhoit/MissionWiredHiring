from .db import db


class AcquisitionFacts(db.Model)
    acquisition_date = db.Column(db.Date)
    acquisitions = db.Column(db.Integer)

def to_dict(self)
        return {
            'email': self.email,
            'code': self.code,
            'is_unsub': self.is_unsub,
            'created_dt': self.created_dt,
            'updated_dt': self.updated_dt,
        }